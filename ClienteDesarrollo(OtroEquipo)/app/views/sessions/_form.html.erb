<%= form_for(@misession) do |f| %>

  <div class="field">
    <%= f.label :nick_name %><br />
    <%= f.text_field :nick_name, :required => true %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, :required => true%>
  </div>
  <div class="actions">
    <%= f.submit 'Iniciar', :class =>"classname"%>
  </div>
<% end %>
