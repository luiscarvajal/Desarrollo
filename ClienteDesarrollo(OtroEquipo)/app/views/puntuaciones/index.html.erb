<h1>Listing puntuaciones</h1>

<table>
  <tr>
    <th>Me gusta</th>
    <th>No me gusta</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @puntuaciones.each do |puntuacione| %>
  <tr>
    <td><%= puntuacione.me_gusta %></td>
    <td><%= puntuacione.no_me_gusta %></td>
    <td><%= link_to 'Show', [@user,@comentario,puntuacione] %></td>
    <td><%= link_to 'Edit', edit_user_comentario_puntuacione_path(@user,@comentario,puntuacione) %></td>
    <td><%= link_to 'Destroy', [@user,@comentario,puntuacione], confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Puntuacione', new_user_comentario_puntuacione_path(@user,@comentario) %>
